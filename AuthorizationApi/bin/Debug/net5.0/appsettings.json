{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"TokenConfig": {
		"SigningKey": "CollateralLoansAuthorizationSigningKey",
		"SecurityAlgorithm": "HS256",
		"SecurityAlgorithmSignature": "http://www.w3.org/2001/04/xmldsig-more#hmac-sha256",
		"AccessTokenExpiresIn": 60,
		"RefreshTokenExpiresIn": 300
	}
}
